package com.ucap.cloud.business.formserver.cssmanager.css.until;
/**
 * @ClassName Ifboolean
 * @Description TODO <P>判断参数是否为空</P>
 * @author  pzg
 * @date 2012-8-19
 */
public class Ifboolean {

	public Ifboolean(){
		
	}
	/**
	 * @param o 要验证的参数
	 * @return 返回对象是否为空
	 */
	public boolean getobj(Object o){
		boolean b = false;
		if(o!=null){
			return b;
		}else{
			return !b;
		}
	}
	/**
	 * 
	 * @param o  要验证的参数
	 * @param o1 要验证的参数
	 * @return  对象是否为空
	 */
	public boolean gettwoObj(Object o,Object o1){
		boolean b = false;
		if(o!=null&&o1!=null){
			return b;
		}else{
			return !b;
		}
	}
}
